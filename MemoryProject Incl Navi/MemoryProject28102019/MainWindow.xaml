<Window x:Class="MemoryProject28102019.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MemoryProject28102019"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="BackGround.png" Stretch="UniformToFill"/>
        </Grid.Background>

        <Frame x:Name="Main" Margin="0,35,0,0" NavigationUIVisibility="Hidden"/>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
        <Button Content="1 Speler Spel" Margin="0,0,0,0" Click="Button_Click" Height="35" Width="100"/>
            <Button Content="2 Speler Spel" Margin="0,0,0,0" Height="35" Width="100" Click="Button_Click_1"/>
            <Button Content="HighScores" Margin="0,0,0,0" Width="100" Height="35"/>
            <Button Content="Home" Margin="0,0,0,0" Width="100" Height="35" Click="Button_Click_2"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Window>
